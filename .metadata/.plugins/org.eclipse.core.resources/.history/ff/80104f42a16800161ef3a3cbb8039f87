package abstraction;

public class CrewMember extends EmployeeClass{
	
	
	
	public CrewMember(String name,String id,String address,int age){
		super(name, id, address, age);
	}
	
	public String getname(){
		return name;
	}
	public String getid(){
		return id;
	}
	public String getaddress(){
		return address;
	}
	public int getage(){
		return age;
	}

}
