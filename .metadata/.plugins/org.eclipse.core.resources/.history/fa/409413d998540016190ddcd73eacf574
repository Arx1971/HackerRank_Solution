package InputStream;

import java.io.File;
import java.io.FileInputStream;

public class Main {
	
	public static void main(String args[]){
		
		FileInputStream fin = null;
		
		try {
			fin = new FileInputStream("home:\\rahin\\Desktop\\rahin.txt");
			
			int i=0;
			
			while((i=fin.read())!=-1){
				System.out.println((char)i);
			}
			
		} catch (Exception e) {
			// TODO: handle exception
		}
		
	}
	
}
