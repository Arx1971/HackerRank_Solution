package encapsulation;

public class StudentClass {
		
	public String name;
	public String address;
	private String id;
	public int age;
	
	
	public StudentClass(String name,String address,int age){
		
		this.age = age;
		this.name = name;
		this.address = address;
		
	}
	
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	
	
	
}
