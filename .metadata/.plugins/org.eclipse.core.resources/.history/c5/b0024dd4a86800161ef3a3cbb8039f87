package abstraction;

public class ManagerClass extends EmployeeClass {

	public ManagerClass(){
		
	}
	
	public ManagerClass(String name,String address,String id, int age){
		super(name, id, address, age);
	}
	public String getname(){
		return name;
	}
	
	public String getid(){
		return id;
	}
	
	public String getaddress(){
		return address;
	}
	
	public int getage(){
		return age;
	}
	
	public void crew_name(){
		System.out.println("Manager Name: " + getname());
	}
	public void crew_address(){
		System.out.println("Manager Address: " + getaddress());
	}
	public void crew_id(){
		System.out.println("Manager Id: " + getid());
	}
	public void crew_age(){
		System.out.println("Manager Age: " + getage());
	}
	
}
