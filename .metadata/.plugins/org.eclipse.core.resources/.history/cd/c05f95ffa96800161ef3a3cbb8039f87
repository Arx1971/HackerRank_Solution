package abstraction;

public class Main {

	public static void main(String args[]){
		
		EmployeeClass crew = new CrewMember() ;
		
		String name = "Mike";
		String address = "United States";
		String id = "Star Wars";
		int age = 24;
		
		
		crew = new CrewMember(name,id,address,age);
		
		crew.crew_name();
		crew.crew_id();
		crew.crew_address();
		crew.crew_age();
		
	}
	
}
